{
    "name": "selfiq/liveproto-bot",
    "description": "Self-bot deployment on Koyeb using LiveProto",
    "type": "project",
    "require": {
        "php": ">=8.2",
        "taknone/liveproto": "^0.0.12",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "taknone/bootstrapper": true
        }
    }
}
